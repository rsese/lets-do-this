<section id="main" class="signup">
  <h1>Sign Up</h1>
  <%= form_for @user, :html => { :class => "std-form" } do |f| %>
    <%= render 'shared/error_messages', :object => f.object  %>
    <%= render 'fields', :f => f %>
    <div class="submit-action">
      <%= f.submit "Sign up", :class => "std-button" %>
    </div>
  <% end %>
</section>
